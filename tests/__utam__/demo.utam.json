{
    "selector": {
        "css": "html"
    },
    "root": true,
    "beforeLoad": [
        {
            "element": "document",
            "apply": "waitForDocumentReady"
        },
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "isPresent"
                        },
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": ".indicator"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "userNameInput",
            "type": ["editable", "clickable"],
            "selector": {
                "css": "dsadsads"
            }
        }
    ],
    "methods": [
        {
            "name": "login",
            "compose": [
                {
                    "element": "userNameInput",
                    "apply": "click"
                }
            ]
        }
    ]
}