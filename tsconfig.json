{
    "extends": "@tsconfig/node20/tsconfig.json",
    "include": [
        "spec/*"
    ],
    "compilerOptions": {
        "allowJs": true,
        "strict": false,
        "allowImportingTsExtensions": true,
        "types": [
           "node",
            "@types/jasmine",
            "@wdio/globals/types",
            "@wdio/jasmine-framework",
            "wdio-utam-service"
        ],
        "paths": {
            "#utam-pageObjects/*": [
                "./pageObjects/*"
            ]
        }
    }
}